
// CRIANDO UMA DATA BASE : 

CREATE DATABASE "Alura"
    WITH
    OWNER = postgres
    ENCODING = 'UTF8'
    LOCALE_PROVIDER = 'libc'
    CONNECTION LIMIT = -1
    IS_TEMPLATE = False;

// Types no Portgres :

https://www.postgresql.org/docs/current/datatype.html
// Types masis usados ,: 
integer
real
serial
numeric

varchar(n)
char(n)
text

boolean

date
time
timestamp
